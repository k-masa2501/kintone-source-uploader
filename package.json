{
    "name": "kintone-source-uploader",
    "version": "2.1.0",
    "description": "This npm package can upload kintone application customization source code, kintone portal customization source code and kintone plugin.",
    "main": "dist/src/index.js",
    "scripts": {
        "build": "tsc",
        "test": "nyc node_modules/.bin/mocha --recursive test/*.test.js"
    },
    "author": "k-masa2501",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/k-masa2501/kintone-source-uploader.git"
    },
    "bin": {
        "source-uploader": "bin/cli.js"
    },
    "nyc": {
        "include": [
            "bin/*.js",
            "dist/*.js"
        ],
        "exclude": [
            "test/*"
        ],
        "reporter": [
            "text-summary",
            "lcov",
            "cobertura"
        ],
        "cache": true,
        "all": true,
        "report-dir": "./coverage"
    },
    "dependencies": {
        "btoa": "^1.2.1",
        "chalk": "^2.4.1",
        "fs": "0.0.1-security",
        "inquirer": "^6.5.2",
        "jsonschema": "^1.4.0",
        "meow": "^7.1.1",
        "os": "^0.1.1",
        "os-locale": "^3.0.1",
        "puppeteer": "^1.20.0",
        "request-promise": "^4.2.6"
    },
    "devDependencies": {
        "@types/mocha": "^8.2.0",
        "@types/node": "^14.14.27",
        "chai": "^4.2.0",
        "mocha": "^8.2.1",
        "nyc": "^14.1.1",
        "path": "^0.12.7",
        "sinon": "^7.5.0",
        "typescript": "^1.8.10"
    }
}
