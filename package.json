{
    "name": "kintone-source-uploader",
    "version": "0.0.10",
    "description": "This npm package can upload kintone application customization source code, kintone portal customization source code and kintone plugin.",
    "main": "dist/src/index.js",
    "scripts": {
        "test": "nyc node_modules/.bin/mocha --reporter spec  --recursive test/*.test.js"
    },
    "author": "k-masa2501",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/k-masa2501/kintone-source-uploader.git"
    },
    "bin": {
        "source-uploader": "bin/cli.js"
    },
    "nyc": {
        "include": [
            "bin/*.js",
            "dist/*.js"
        ],
        "exclude": [
            "test/*"
        ],
        "reporter": [
            "text-summary",
            "lcov"
        ],
        "cache": true,
        "all": true,
        "report-dir": "./test_report/coverage"
    },
    "dependencies": {
        "await-semaphore": "^0.1.3",
        "btoa": "^1.2.1",
        "chalk": "^2.4.1",
        "form-data": "^2.3.3",
        "fs": "0.0.1-security",
        "inquirer": "^6.2.1",
        "jsonschema": "^1.2.4",
        "meow": "^5.0.0",
        "os": "^0.1.1",
        "os-locale": "^3.0.1",
        "puppeteer": "^1.11.0",
        "request": "^2.88.0",
        "util": "^0.11.1"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "mocha": "^6.0.2",
        "nyc": "^13.3.0",
        "path": "^0.12.7",
        "sinon": "^7.2.5"
    }
}
