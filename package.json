{
    "name": "kintone-source-uploader",
    "version": "3.0.0",
    "description": "This npm package can upload kintone application customization source code, kintone portal customization source code and kintone plugin.",
    "main": "dist/src/index.js",
    "scripts": {
        "build": "tsc",
        "test": "nyc node_modules/.bin/mocha --recursive test/*.test.js"
    },
    "author": "k-masa2501",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/k-masa2501/kintone-source-uploader.git"
    },
    "bin": {
        "source-uploader": "bin/cli.js"
    },
    "nyc": {
        "include": [
            "bin/*.js",
            "dist/*.js"
        ],
        "exclude": [
            "test/*"
        ],
        "reporter": [
            "text-summary",
            "lcov",
            "cobertura"
        ],
        "cache": true,
        "all": true,
        "report-dir": "./coverage"
    },
    "dependencies": {
        "axios": "^0.27.2",
        "btoa": "^1.2.1",
        "chalk": "^5.0.1",
        "form-data": "^4.0.0",
        "fs": "0.0.1-security",
        "https-proxy-agent": "^5.0.1",
        "inquirer": "^8.2.4",
        "jsonschema": "^1.4.1",
        "meow": "^10.1.2",
        "os": "^0.1.2",
        "os-locale": "^6.0.2",
        "puppeteer": "^14.4.0",
        "qs": "^6.10.5"
    },
    "devDependencies": {
        "@types/mocha": "^9.1.1",
        "@types/node": "^17.0.42",
        "chai": "^4.3.6",
        "mocha": "^10.0.0",
        "nyc": "^15.1.0",
        "path": "^0.12.7",
        "sinon": "^14.0.0",
        "typescript": "^4.7.3"
    },
    "type": "module"
}
